                                <owl:allValuesFrom rdf:resource="http://www.cs.teilar.gr/ontologies/KObject.owl#PhysicalLocation"/>
                            </owl:Restriction>
                            <owl:Restriction>
                                <owl:onProperty rdf:resource="http://www.cs.teilar.gr/ontologies/Graph.owl#hasAssociated"/>
                                <owl:allValuesFrom rdf:resource="http://www.cs.teilar.gr/ontologies/LearningObject.owl#LearningObject"/>
                            </owl:Restriction>
